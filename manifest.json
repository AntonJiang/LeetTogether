{
  "manifest_version": 2,

  "name": "LeetTogether",
  "description": "This extension will help you LeetCode with friends",
  "version": "0.0",

  "browser_action": {
   "default_icon": "leet_code_icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "cookies",
   "*://leetcode.com/"
   ]
}